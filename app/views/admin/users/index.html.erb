<p id="notice"><%= notice %></p>

<h1><%= t("users.list_users") %></h1>

<%= search_form_for [:admin, @q], remote: true do |f| %>

  <%= f.label t "users.name" %>
  <%= f.text_field :name_cont %>

  <%= f.submit %>
<% end %>
<%= link_to t("users.add_user"), new_admin_user_path, class: "btn btn-primary" %>

<div id="result">
  <%= render "index", user: @user %>
</div>
